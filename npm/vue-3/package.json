{
  "name": "@cypress/vue-3",
  "version": "0.0.0-development",
  "description": "Browser-based Component Testing for Vue 3 with Cypress.io ✌️🌲",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build-prod": "yarn build",
    "cy:open": "node ../../scripts/cypress.js open-ct --project ${PWD}",
    "cy:run": "node ../../scripts/cypress.js run-ct --project ${PWD}",
    "test": "yarn cy:run",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@vue/test-utils": "^2.0.0-rc.0",
    "debug": "^4.3.2"
  },
  "devDependencies": {
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "3.x"
  }
}